    "bundle": "NODE_ENV=production npx webpack --config webpack.config.js"
  },
  "author": "Ben Neigher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bneigher/template-module-webpack.git"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "es6-promise": "^4.1.1",
    "eslint": "^4.6.0",
    "eslint-config-hapi": "^10.1.0",
    "eslint-plugin-hapi": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
