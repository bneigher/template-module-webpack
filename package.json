{
  "name": "template-module-webpack",
  "version": "1.0.1",
  "description": "The one stop shop for modern javascript library development",
  "main": "index.js",
  "webpack": "./dist/bundle.js",
  "browser": {
    "index.js": "./dist/web-bundle.js"
  },
  "module": "src",
  "scripts": {
    "dev": "NODE_ENV=development npx webpack-dev-server --config webpack.config.js",
