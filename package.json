{
  "name": "template-module-webpack",
  "version": "1.2.0",
  "description": "The one stop shop for modern javascript library development",
  "main": "index.js",
  "webpack": "./src/index.js",
  "browser": {
    "index.js": "./dist/bundle.js"
  },
  "module": "src",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config webpack.config.js",
    "test": "cross-env NODE_ENV=testing ./node_modules/.bin/mocha --require babel-core/register",
    "bundle": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js"
  },
  "author": "Ben Neigher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bneigher/template-module-webpack.git"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/polyfill": "7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/register": "^7.0.0-beta.47",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "es6-promise": "^4.1.1",
    "eslint-config-hapi": "^12.0.0",
    "eslint-plugin-hapi": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "stylelint-webpack-plugin": "^0.10.0",
    "webpack": "^4.8.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.x",
    "webpack-dev-server": "^3.1.0"
  },
  "keywords": [
    "babel",
    "hmr",
    "hot-module-replacement",
    "es6",
    "sass",
    "library",
    "workflow",
    "webpack"
  ],
  "dependencies": {}
