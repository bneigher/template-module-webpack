    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint-selector-bem-pattern": "^1.1.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "babel": {
    "presets": [
      "es2015"
