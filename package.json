{
  "name": "template-module-webpack",
  "version": "1.3.0",
  "description": "The one stop shop for modern javascript library development",
  "main": "./dist/bundle.js",
  "webpack": "./src/index.js",
  "browser": {
    "index.js": "./dist/bundle.js"
  },
  "module": "src",
  "scripts": {
    "autotest": "cross-env NODE_ENV=test jest --watch --coverage",
    "bundle": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config webpack.config.js",
    "document": "jsdoc2md -f 'src/**/*.js' > doc/README.md",
    "test": "cross-env NODE_ENV=test jest --coverage"
  },
  "author": "Ben Neigher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bneigher/template-module-webpack.git"
  },
  "devDependencies": {
    "@babel/core": "7.x.x",
    "@babel/helper-plugin-utils": "7.x.x",
    "@babel/plugin-transform-runtime": "7.0.x",
    "@babel/polyfill": "7.x.x",
    "@babel/preset-env": "7.0.x",
    "@babel/register": "7.0.x",
    "@babel/runtime": "7.0.x",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.x.0",
    "babel-loader": "8.0.x",
    "babel-plugin-add-module-exports": "1.0.x",
    "babel-plugin-syntax-async-functions": "6.13.x",
    "babel-plugin-transform-class-properties": "6.24.x",
    "cross-env": "5.2.x",
    "css-loader": "1.0.x",
    "es6-promise": "4.2.x",
    "eslint": "5.x.x",
    "eslint-config-hapi": "12.0.x",
    "eslint-plugin-hapi": "4.1.x",
    "eslint-plugin-jest": "21.22.x",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "jest": "23.x.0",
    "jsdoc-to-markdown": "4.x.x",
    "node-sass": "4.9.x",
    "normalize.css": "8.0.x",
    "optimize-css-assets-webpack-plugin": "5.0.x",
    "postcss-import": "12.0.x",
    "postcss-loader": "3.0.x",
    "sass-loader": "7.1.x",
    "style-loader": "0.23.x",
    "stylelint-selector-bem-pattern": "2.0.x",
    "stylelint-webpack-plugin": "0.10.x",
    "webpack": "4.19.x",
    "webpack-bundle-analyzer": "3.0.x",
    "webpack-cli": "3.1.x",
    "webpack-dev-server": "3.1.x"
  },
  "keywords": [
    "babel",
    "hmr",
    "hot-module-replacement",
    "es6",
    "sass",
    "library",
    "workflow",
    "webpack"
  ],
  "dependencies": {}
}
